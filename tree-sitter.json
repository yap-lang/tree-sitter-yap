{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "yap",
      "camelcase": "Yap",
      "title": "YAP! Programming Language",
      "scope": "source.yap",
      "file-types": [
        "yap"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "tags": [
        "queries/tags.scm"
      ],
      "injection-regex": "^yap$",
      "class-name": "TreeSitterYap"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "Apache-2.0",
    "description": "YAP! grammar for tree-sitter",
    "authors": [
      {
        "name": "Valaphee",
        "email": "valaphee@valaphee.com",
        "url": "https://valaphee.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/yap-lang/tree-sitter-yap"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
